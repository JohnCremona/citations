@build_mk@

SOURCES		= bib2html.cpp bib2ref.cpp 


PROGS		= $(SOURCES:.cpp=$(SUFFIX).exe)

all install: directory $(B2H_LIBNAME) $(PROGS)

@directory_mk@

@bin_mk@

$(B2H_LIBNAME):
	cd $(B2H_DIR_SRC)/database ; $(MAKE)
	cd $(B2H_DIR_SRC)/field ; $(MAKE)
	cd $(B2H_DIR_SRC)/config ; $(MAKE)
	cd $(B2H_DIR_SRC)/entry ; $(MAKE)
	cd $(B2H_DIR_SRC)/tools ; $(MAKE)

uninstall:
	rm -f $(PROGS)

@clean_mk@

