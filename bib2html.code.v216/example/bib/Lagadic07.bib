@Article{Mansard07e,
  author =	 {Mansard, N. and Chaumette, F.},
  title =	 {Task sequencing for sensor-based control},
  journal =	 {IEEE Trans. on Robotics},
  year =	 2007,
  doi =		 {http://dx.doi.org/10.1109/TRO.2006.889487},
  volume =	 23,
  number =	 1,
  pages =	 {60-72},
  month =	 feb,
  abstract =	 {Classical sensor-based approaches tend to constrain
                  all the degrees of freedom of a robot during the
                  execution of a task. In this paper, a new solution
                  is proposed. The key idea is to divide the global
                  full-constraining task into several subtasks, which
                  can be applied or inactivated to take into account
                  potential constraints of the environment. Far from
                  any constraint, the robot moves according to the
                  full task. When it comes closer to a configuration
                  to avoid, a higher level controller removes one or
                  several subtasks, and activates them again when the
                  constraint is avoided. The last controller ensures
                  the convergence at the global level by introducing
                  some look-ahead capabilities when a local minimum is
                  reached. The robot accomplishes the global task by
                  automatically sequencing sensor-based tasks,
                  obstacle avoidance, and short deliberative
                  phases. In this paper, a complete solution to
                  implement this idea is proposed, along with several
                  experiments that prove the validity of this
                  approach.},
  url =
                  {http://www.irisa.fr/lagadic/pdf/2007_itro_mansard.pdf},
  x-editorial-board ={yes},
  x-international-audience ={yes}
}

@InProceedings{Marchand07a,
  author =	 {Marchand, E. and Chaumette, F.},
  title =	 {Fitting 3D Models on Central Catadioptric Images},
  booktitle =	 {IEEE Int. Conf.  on Robotics and Automation,
                  ICRA'07},
  pages =	 {52-58},
  year =	 2007,
  address =	 {Rome, Italy},
  adresse =	 {Rome, Italie},
  keyword =	 {icra07},
  url =
                  {http://www.irisa.fr/lagadic/pdf/2007_icra_marchand_omni.pdf},
  month =	 apr,
  abstract =	 { Increasing the field of view of camera is an
                  important issue practical in robot vision. One
                  solution is to consider catadioptric camera that
                  allows a 360o field of view. In this paper we
                  propose a 3D model tracking algorithm that allows a
                  fast and reliable tracking of 3D objects within
                  central catadioptric images. The proposed approach
                  relies on the virtual visual servoing approach. All
                  the modeling aspects have been reconsidered to
                  consider the projection model. Results show the
                  method to be robust and efficient.},
  doi =		 {http://dx.doi.org/10.1109/ROBOT.2007.363764 },
  x-editorial-board ={yes},
  x-proceedings ={yes},
  x-international-audience ={yes}
}

@InProceedings{Marchand07b,
  author =	 {Marchand, E.},
  title =	 {Control Camera and Light Source Positions using
                  Image Gradient Information},
  booktitle =	 {IEEE Int. Conf.  on Robotics and Automation,
                  ICRA'07},
  year =	 2007,
  pages =	 {417-422},
  url =
                  {http://www.irisa.fr/lagadic/pdf/2007_icra_marchand_light.pdf},
  address =	 {Rome, Italy},
  adresse =	 {Rome, Italie},
  keyword =	 {icra07},
  month =	 apr,
  abstract =	 { In this paper, we propose an original approach to
                  control camera position and/or lightning conditions
                  in an environment using image gradient
                  information. Our goal is to ensure a good viewing
                  condition and good illumination of an object to
                  perform vision-based task (recognition, tracking,
                  etc.). Within the visual servoing framework, we
                  propose solutions to two different issues:
                  maximizing the brightness of the scene and
                  maximizing the contrast in the image.  Solutions are
                  proposed to consider either a static light and a
                  moving camera, either a moving light and a
                  static/moving camera. The proposed method is
                  independent of the structure, color and aspect of
                  the objects.  Experimental results on both synthetic
                  and real images are finally presented.},
  doi =		 {http://dx.doi.org/10.1109/ROBOT.2007.363822 },
  x-editorial-board ={yes},
  x-proceedings ={yes},
  x-international-audience ={yes}
}

