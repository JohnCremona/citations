@InProceedings{Dune07a,
  author =	 {Dune, C. and Marchand, E. and Leroux, C.},
  title =	 {One Click Focus with Eye-in-hand/Eye-to-hand
                  Cooperation},
  booktitle =	 {IEEE Int. Conf.  on Robotics and Automation,
                  ICRA'07},
  year =	 2007,
  address =	 {Rome, Italy},
  adresse =	 {Rome, Italie},
  pages =	 {2471-2476},
  url =		 {http://www.irisa.fr/lagadic/pdf/2007_icra_dune.pdf},
  month =	 apr,
  abstract =	 { A critical assumption of many multi-view control
                  systems is the initial visibility of the regions of
                  interest from all the views. In this paper proposes
                  an initialization step for an hybrid
                  eye-in-hand/eye-to-hand grasping system to overcome
                  this requierement. In this paper, the object of
                  interest is assumed to be within the eye-to-hand
                  field of view, whereas it may not be within the
                  eye-in-hand one. The object model is unknown and no
                  database is used. The object lies in a complex scene
                  with a cluttered background. We present a method to
                  automatically focus the object of interest. The
                  method is tested and validated on a multi view
                  robotic system.},
  keyword =	 {icra07},
  doi =		 {http://dx.doi.org/10.1109/ROBOT.2007.363690},
  x-editorial-board ={yes},
  x-proceedings ={yes},
  x-international-audience ={yes}
}

